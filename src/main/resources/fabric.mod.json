{
  "schemaVersion": 1,
  "id": "buildcraft",
  "version": "${version}",
  "name": "Buildcraft",
  "description": "Extending Minecraft with pipes, auto-crafting, quarries, engines and much more!\nThis is a part port and part reimplementation of various Buildcraft versions aiming for feature parity with Buildcraft for Minecraft r1.2.5",
  "authors": [
    "ralf2oo2",
    "DanyGames2014"
  ],

  "license": "MMPLv1",
  "icon": "assets/buildcraft/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.danygames2014.buildcraft.Buildcraft",
      "net.danygames2014.buildcraft.init.InitListener",
      "net.danygames2014.buildcraft.init.FluidListener",
      "net.danygames2014.buildcraft.init.BlockEntityListener",
      "net.danygames2014.buildcraft.init.EntityListener",
      "net.danygames2014.buildcraft.init.PacketTypeListener",
      "net.danygames2014.buildcraft.init.PluggableListener",
      "net.danygames2014.buildcraft.init.AfterBlockAndItemListener"
    ],
    "stationapi:event_bus_client": [
      "net.danygames2014.buildcraft.init.ScreenHandlerListener",
      "net.danygames2014.buildcraft.init.BlockEntityRendererListener",
      "net.danygames2014.buildcraft.init.TextureListener",
      "net.danygames2014.buildcraft.init.EntityRendererListener"
    ],
    "stationapi:event_bus_server": [
    ],
    "buildcraft:event_bus": [
      "net.danygames2014.buildcraft.init.RecipeListener"
    ]
  },
  "mixins": [
    "buildcraft.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
